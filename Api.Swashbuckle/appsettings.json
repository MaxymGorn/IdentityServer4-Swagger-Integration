{
  "Logging": {
    "LogPath": "Logs//file.log",
    "SizeLimit": 300000000,
    "FileCountLimit": 4,
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "IdentityServerSettings": {
    "Authority": "https://localhost:6001",
    "ApiName": "api1"
  },
  "SwaggerGenSettings": {
    "SwaggerDoc": {
      "name": "v1",
      "OpenApiInfo": {
        "Description": "Project description",
        "Title": "Protected API - {0}",
        "Version": "your version will be internally replaced",
        "TermsOfService": "tos@url.com",
        "Contact": {
          "Email": "contact@mail.com",
          "Name": "Maxs",
          "Url": "contact@url.com"
        },
        "License": {
          "Name": "GPL",
          "Url": "lic@url.com"
        }
      }
    }
  },
  //Generate pfx cert: 'dotnet dev-certs https -v -ep ${HOME}/.aspnet/https/aspnetapp.pfx -p 12345678 'and copy to this directory
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5001",
        "Certificate": {
          "Path": "aspnetapp.pfx",
          "Password": "12345678"
        }
      }
    }
  }
}
